// Hello, world!
// A
// B
// C
fn main() -> i32 {
    print "Hello, world!";
    abc(1);
    return 0;
}

fn abc(n: i32) -> i32 {
    if n == 1 {
        print "A";
        abc(n + 1);
    } elif n == 2 {
        print "B";
        abc(n + 1);
    } elif n == 3 {
        print "C";
        abc(n + 1);
    }

    return 0;
}
